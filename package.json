{
  "name": "katextual",
  "version": "0.1.2",
  "description": "Renders compatible TeX commands using KaTeX to images. ",
  "main": "./dist/katextual.js",
  "scripts": {
    "build": "tsc && npm run copy-files",
    "copy-files": "npx cpy src/puppeteerTemplate.hbs dist",
    "test": "ts-mocha tests/**/*.test.ts --exit",
    "coverage": "nyc npm test",
    "lint": "eslint \"**\\src\\*.ts\""
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GreenCappuccino/KaTextual.git"
  },
  "keywords": [
    "tex",
    "latex",
    "katex",
    "png"
  ],
  "author": "Brian Lu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GreenCappuccino/KaTextual/issues"
  },
  "homepage": "https://github.com/GreenCappuccino/KaTextual#readme",
  "devDependencies": {
    "@types/katex": "^0.11.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.1",
    "@types/puppeteer": "^5.4.4",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "cpy-cli": "^3.1.1",
    "eslint": "^7.32.0",
    "eslint-plugin-promise": "^5.1.0",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "ts-mocha": "^8.0.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "handlebars": "^4.7.7",
    "katex": "^0.13.18",
    "log4js": "^6.3.0",
    "puppeteer": "^10.2.0"
  }
}
